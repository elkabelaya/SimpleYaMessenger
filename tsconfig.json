{
  "compilerOptions":
  {
    "moduleResolution":"node",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "alwaysStrict": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "target": "es6",
    "outDir": "static/js",
    "allowJs": true,
    "plugins": [{ "name": "typescript-plugin-css-modules" }],
    "types": [
      "jest"
    ]
  },
  "include": ["src/**/*"],

}
